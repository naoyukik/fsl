    "keywords": [
        "framework",
        "micro",
        "api",
        "router",
        "microservices",
        "docker",
        "php"
    ],
    "homepage": "https://github.com/yesinteractive/fsl",
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "ext-fileinfo": "*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Yes Interactive",
            "email": "411@yes-interactive.com"
        },
        {
            "name": "naoyukik"
        }
    ],
    "autoload": {
        "files": [
            "lib/fsl.php"
        ],
        "classmap": [
            "controllers",
            "lib"
        ]
    },
    "minimum-stability": "dev",
    "require-dev": {
        "josegonzalez/dotenv": "dev-master",
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^9.0"
    }
}
